apiVersion: kustomize.config.k8s.io/v1beta1
kind: Kustomization
namespace: op-connect-operator
resources:
- namespace.yaml
secretGenerator:
- name: op-credentials-kustomize
  files:
  - 1password-credentials.json
- name: onepassword-token-kustomize
  files:
  - token
generatorOptions:
  disableNameSuffixHash: true
